{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "Node",
        "noUnusedLocals": true,
        "declaration": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "ES2020",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": "./",
        "incremental": true,
        "esModuleInterop": true
    },
    "exclude": ["node_modules", "dist"]
}